    <rule-file/>
    <rule-definition>package org.pentaho.di.trans.steps.rules&#xd;&#xa; &#xd;&#xa;import org.pentaho.di.trans.steps.rules.Rules.Column&#x3b;&#xd;&#xa;&#xd;&#xa;global java.util.List list&#xd;&#xa; &#xd;&#xa;rule &#x22;Get first name&#x22;&#xd;&#xa;    dialect &#x22;mvel&#x22;&#xd;&#xa;	when&#xd;&#xa;		Column&#x28; name &#x3d;&#x3d; &#x22;nameFirst&#x22;, &#x24;nameFirst &#x3a; payload&#x29;&#xd;&#xa;	then&#xd;&#xa;	    System.out.println&#x28; &#x22;First Name&#x22; &#x2b;&#x24;nameFirst &#x29;&#x3b;&#xd;&#xa;end&#xd;&#xa;&#xd;&#xa;rule &#x22;Get last name&#x22;&#xd;&#xa;    dialect &#x22;mvel&#x22;&#xd;&#xa;	when&#xd;&#xa;		Column&#x28; name &#x3d;&#x3d; &#x22;nameLast&#x22;, &#x24;nameLast &#x3a; payload&#x29;&#xd;&#xa;	then&#xd;&#xa;	    System.out.println&#x28;  &#x22;Last Name&#x22; &#x2b;&#x24;nameLast &#x29;&#x3b;&#xd;&#xa;end&#xd;&#xa;&#xd;&#xa;rule &#x22;Compile name&#x22;&#xd;&#xa;    dialect &#x22;mvel&#x22;&#xd;&#xa;    when&#xd;&#xa;        &#x24;nameFirst &#x3a; Column&#x28;name &#x3d;&#x3d; &#x22;nameFirst&#x22;&#x29;&#xd;&#xa;        &#x24;nameLast &#x3a; Column&#x28;name &#x3d;&#x3d; &#x22;nameLast&#x22;&#x29;&#xd;&#xa;    then&#xd;&#xa;        System.out.println&#x28;&#x22;User&#x3a; &#x22; &#x2b; &#x24;nameFirst.payload &#x2b; &#x22; &#x22; &#x2b; &#x24;nameLast.payload&#x29;&#x3b;&#xd;&#xa;end&#xd;&#xa;&#xd;&#xa;rule &#x22;Decline&#x22;&#xd;&#xa;    dialect &#x22;mvel&#x22;&#xd;&#xa;    when&#xd;&#xa;        &#x24;fica &#x3a; Column&#x28;name &#x3d;&#x3d; &#x22;fica&#x22;, payload &#x3c; 550&#x29;&#xd;&#xa;        &#xd;&#xa;    then&#xd;&#xa;        Column approvalStatus &#x3d; new Column&#x28;&#x29;&#x3b;&#xd;&#xa;        approvalStatus.name &#x3d; &#x22;approvalStatus&#x22;&#xd;&#xa;        approvalStatus.type &#x3d; String.class&#xd;&#xa;        approvalStatus.payload &#x3d; &#x22;declined&#x22;&#xd;&#xa;        &#xd;&#xa;        Column approvalClassification &#x3d; new Column&#x28;&#x29;&#x3b;&#xd;&#xa;        approvalClassification.name &#x3